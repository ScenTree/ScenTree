<!-- LAYERS -->
<Layer name="clade1" status="on" srs="&osm2pgsql_projection;">
  <StyleName>clade1</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way, clade,zoomview,ref, nbdesc,taxid,
      (CASE WHEN (color = '' OR color = 'None' OR color IS NULL) THEN '#FFFFFF' ELSE
      color END) AS color, NULL AS common_name
      from polygons
      where ref=1
      order by taxid DESC
    ) as RES</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>
<Layer name="branches1" status="on" srs="&osm2pgsql_projection;">
  <StyleName>branches1</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,branch,zoomview, ref, NULL AS common_name
      from lines
      where branch is true AND zoomview &gt; 3
      ) as roads
    </Parameter>
    &datasource-settings;
  </Datasource>
</Layer>
<Layer name="Clade_names" status="on" srs="&osm2pgsql_projection;">
  <StyleName>Clade_names</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,cladecenter,zoomview,sci_name_EN AS sci_name, nbdesc,taxid, NULL AS common_name
      from points
      where cladecenter is true
      order by taxid DESC
    ) as placenames</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>
<Layer name="Node_names" status="on" srs="&osm2pgsql_projection;">
  <StyleName>Node_names</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,tip,zoomview,sci_name_EN AS sci_name, nbdesc,taxid, NULL AS common_name
      from points
      where cladecenter is null and tip &lt; 1
      order by taxid DESC
    ) as points</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>
<Layer name="tip_names" status="on" srs="&osm2pgsql_projection;">
  <StyleName>tip_names</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,sci_name_EN AS sci_name, tip,zoomview, nbdesc, NULL AS common_name
      from points where tip &gt; 0
    ) as placenames</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>
</Map>
